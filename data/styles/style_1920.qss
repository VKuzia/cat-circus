QPushButton {
  background-color: rgba(255, 198, 25, 0.85);
  color: rgb(146, 12, 204);
  border-style: outset;
  border-color: rgba(255, 246, 232, 1);
  border-width: 9px;
  border-radius: 37px;
  padding: 8px;
  
  
  font-family: Verdana;
  font-size: 43px;
  font-weight: Bold;
  font-style: italic
}

QPushButton:hover {
  background-color: rgba(255, 198, 25, 1);
  border-color: rgba(255, 246, 232, 1);
}

QPushButton:pressed {
  background-color: rgba(240, 183, 10,1);
  border-color: rgba(240, 231, 218, 1);
}

